{
  "name": "@massimo-cassandro/json-table-react",
  "version": "0.2.1",
  "description": "Html table builder from json (react)",
  "main": "dist/index.js",
  "homepage": ".",
  "scripts": {
    "start": "PORT=3501 react-scripts start",
    "start (FF)": "BROWSER=Firefox PORT=3501 react-scripts start",
    "build": "react-scripts build",
    "make-dist": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files && cp -R src/src dist",
    "upd vers": "npx update-version",
    "local server (esm demo)": "python3 -m http.server",
    "npm publish": "npm publish",
    "upd@m": "npx upd@m",
    "npm update": "npm update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/massimo-cassandro/jsonTable-react.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "HTML",
    "table",
    "json",
    "ajax"
  ],
  "stylelint": {
    "extends": "@massimo-cassandro/linters-config/stylelintrc.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "./node_modules/@massimo-cassandro/linters-config/eslintrc.js"
    ]
  },
  "files": [
    "dist/**/*.{css,scss,js,jsx,map}"
  ],
  "author": "Massimo Cassandro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/massimo-cassandro/jsonTable/issues"
  },
  "dependencies": {
    "@massimo-cassandro/js-utilities": "^1.1.0",
    "classnames": "^2.3.1",
    "js-file-download": "^0.4.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.56.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@massimo-cassandro/dev-utilities": "^1.10.2",
    "@massimo-cassandro/linters-config": "^1.1.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
